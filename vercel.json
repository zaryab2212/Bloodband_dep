{
    "builds": [
        {
          "src": "server.js",
          "use": "@vercel/node"
        },
        {
          "src": "build/**",
          "use": "@vercel/static"

        }
      ],
    
    "routes": [
 
    
      {
        "src": "/auth/register",
        "dest": "server.js"
      },
      {
        "src": "/auth/login",
        "dest": "server.js"
      },
      {
        "src": "/auth/currentuser",
        "dest": "server.js"
      },
    
    
    
      {
        "src": "/auth(.*)",
        "dest": "server.js"
      },
    
    
    
      {
        "src": "/inventory/create",
        "dest": "server.js"
      },
      {
        "src": "/inventory/all-records",
        "dest": "server.js"
      },
      {
        "src": "/inventory/recent-records",
        "dest": "server.js"
      },
      {
        "src": "/inventory/all-records-consumer",
        "dest": "server.js"
      },
      {
        "src": "/inventory/donors",
        "dest": "server.js"
      },
      {
        "src": "/inventory/hospitals",
        "dest": "server.js"
      },
      {
        "src": "/inventory/organizations",
        "dest": "server.js"
      },
      {
        "src": "/inventory/org-for-hosp",
        "dest": "server.js"
      },
    
      {
        "src": "/inventory(.*)",
        "dest": "server.js"
      },
      {
        "src": "/analytics/get_bloodgroup",
        "dest": "server.js"
      },
    
      {
        "src": "/analytics(.*)",
        "dest": "server.js"
      },
      {
        "src": "/admin/get-hospital",
        "dest": "server.js"
      },
      {
        "src": "/admin/get-hospital(.*)",
        "dest": "server.js"
      },
      {
        "src": "/admin/get-donor",
        "dest": "server.js"
      },
      {
        "src": "/admin/get-donor(.*)",
        "dest": "server.js"
      },
      {
        "src": "/admin/get-organization",
        "dest": "server.js"
      },
      {
        "src": "/admin/get-organization(.*)",
        "dest": "server.js"
      },
    
      {
        "src": "/admin(.*)",
        "dest": "server.js"
      }
    ]
  }